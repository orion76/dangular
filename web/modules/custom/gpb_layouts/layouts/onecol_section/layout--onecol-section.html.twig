{#
/**
 * @file
 * Default theme implementation for a four-column 25%-25%-25%-25% layout.
 *
 * Available variables:
 * - content: The content for this layout.
 * - attributes: HTML attributes for the layout <div>.
 *
 * @ingroup themeable
 */
#}
{% set classes = [
  'layout',
] %}
{% if content %}

  <div{{ attributes}}>
    {% if content.first %}
        {{ content.first }}
    {% endif %}
  </div>
{% endif %}
